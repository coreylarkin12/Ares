@{
    Layout = null;
}
@{ 
    int f1 = 0, f2 = 1, f3 = 0;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/styles.css" rel="stylesheet" />
    <title></title>
</head>
<body>
    <div>
        <p>Exercise 2 - Fibonacci 25</p>
    </div>

    <ul style="list-style:none">
        
        @for(int i = 0; i < 25; i++)
        {
            f3 = f1 + f2;
            <li>@f3</li>
            f1 = f2;
            f2 = f3;
        }
    </ul>

</body>
</html>
